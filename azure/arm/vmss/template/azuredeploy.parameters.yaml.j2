$schema: https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#
contentVersion: 1.0.0.0
parameters:
  location:
    value: {{ region }}
    #  vmssName:
    # value: {{ vmssName }}
  vmSku:
    value: {{ vmSku }}
  sshPublicKey:
    value: ssh-rsa  azureuser@xdp-standard
  adminUsername:
    value: azureuser
  instanceCount:
    value: {{ instanceCount }}
  singlePlacementGroup:
    value: false
  priority:
    value: Regular
  pipName:
    value: ''
  pipLabel:
    value: ''
  ipAllocationMethod:
    value: Static
  skuType:
    value: Standard
  vnetName:
    value: {{ vnetName }}
  subnetResourceGroup:
    value: {{ subnetResourceGroup }}
  subnetName:
    value: {{ subnet }}
  enableAcceleratedNetworking:
    value: {{ enableAcceleratedNetworking }}
  publicIpAddressPerInstance:
    value: false
  upgradeMode:
    value: Manual
  customData:
    value: 
  image:
    value:
      {{ image }}
      #      publisher: OpenLogic
      #offer: CentOS-CI
      #sku: 7-CI
      #version: latest
  spreadingAlgorithmPlatformFaultDomainCount:
    value: 3
  osDiskSize:
    value: {{ osDiskSize }}
  osDiskType:
    value: StandardSSD_LRS
  loadBalancerFrontEndPort:
    value: {{ lbPort }}
  loadBalancerBackEndPort:
    value: {{ lbPort }}
  overprovision:
    value: false
# vim: ft=yaml.j2
