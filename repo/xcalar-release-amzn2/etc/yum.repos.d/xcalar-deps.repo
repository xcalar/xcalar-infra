[xcalar-deps]
name=Xcalar Dependencies for AMZN2
baseurl=http://repo.xcalar.net/rpm-deps/amzn2/$basearch/
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Xcalar
enabled=0
priority=12

